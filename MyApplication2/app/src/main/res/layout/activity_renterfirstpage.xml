<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.ahnaf.myapplication2.renterfirstpage">
   <TextView
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="CHOOSE YOUR HOUSE"
       android:textSize="25dp"
       android:textStyle="bold"
       android:textColor="@android:color/holo_green_dark"
       android:layout_marginLeft="40dp"
       android:id="@+id/textView7" />
     <EditText
         android:layout_width="match_parent"
         android:layout_height="wrap_content"
         android:text="AREA"
         android:textSize="20dp"
         android:textColor="@android:color/black"
         android:textStyle="bold"
         android:layout_marginTop="60dp"
         android:layout_marginLeft="15dp"
         android:layout_marginRight="15dp"
         android:id="@+id/farea"
         />
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="BEDROOM"
        android:textSize="20dp"
        android:textColor="@android:color/black"
        android:textStyle="bold"
        android:layout_marginTop="120dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:id="@+id/fbed"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Range of rent"
        android:textColor="@android:color/black"
        android:textStyle="bold"
        android:layout_marginTop="180dp"
        android:textSize="25dp"
        android:layout_marginLeft="15dp"
        />
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Range one"
        android:textSize="20dp"
        android:textColor="@android:color/black"
        android:textStyle="bold"
        android:layout_marginTop="220dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:id="@+id/frent1"
        />
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Range two"
        android:textSize="20dp"
        android:textColor="@android:color/black"
        android:textStyle="bold"
        android:layout_marginTop="269dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:id="@+id/frent2"
        />
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="FIND BEST HOUSE"
        android:textSize="20dp"
        android:id="@+id/find1"
        android:layout_marginTop="350dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        />
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="FIND ALL HOUSE"
        android:textSize="20dp"
        android:id="@+id/find2"
        android:layout_marginTop="410dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        />

</RelativeLayout>
